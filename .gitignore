# See https://help.github.com/articles/ignoring-files for more about ignoring files.

# Ignore bundler config.
/.bundle
/vendor/bundle

# Ignore all environment files.
.env*
.env.local
.env.development
.env.test
.env.production

# Ignore Rails master key for decrypting credentials.
/config/master.key

# Ignore log files and temp files.
/log/*
!/log/.keep

/tmp/*
!/tmp/.keep

# Ignore pidfiles but keep directory.
/tmp/pids/*
!/tmp/pids/
/tmp/pids/.keep

# Ignore uploaded files and storage.
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
/tmp/storage/.keep

# Ignore public assets (precompiled)
/public/assets

# Ignore database files (SQLite)
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore Byebug command history file
.byebug_history

# Ignore node modules if using Webpacker or other JS tools
/node_modules/

# Ignore Yarn integrity file
.yarn-integrity

# Ignore editor temp files
*.swp
*.swo

# Windows system files
Thumbs.db
ehthumbs.db

# macOS system files
.DS_Store

# Other temporary files
*.tmp
*.temp
*.log

# Ignore coverage reports (if using SimpleCov or similar)
/coverage/

# Ignore vendor assets (optional, if you vendor assets)
/vendor/assets/

# Ignore Docker files if you don't want to track container images or data volumes
# (only if relevant)
/docker-compose.override.yml
/docker-compose.prod.yml
/docker-compose.dev.yml
